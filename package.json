    "url": "https://github.com/Phicks-debug/linkedin-web-scrapper/issues"
  },
  "homepage": "https://github.com/Phicks-debug/linkedin-web-scrapper#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "playwright": "^1.54.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  }
}